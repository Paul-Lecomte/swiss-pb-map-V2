{
  "name": "swiss_networkd_map",
  "version": "1.0.0",
  "description": "Web app for public transport in SWitzerland",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "update-routes": "node --max-old-space-size=32768 utils/gtfsDataUpdater.js --processedroutes",
    "update-stoptimes": "node --max-old-space-size=32768 utils/gtfsDataUpdater.js --processedstoptimes",
    "update-stops": "node --max-old-space-size=32768 utils/gtfsDataUpdater.js --processedstops",
    "update-gtfs": "node --max-old-space-size=32768 utils/gtfsDataUpdater.js --base",
    "test": "node tests/realtime.spec.js"
  },
  "author": "Lecomte Paul",
  "license": "MIT",
  "dependencies": {
    "@turf/turf": "^7.2.0",
    "axios": "^1.8.1",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "geojson-rbush": "^3.2.0",
    "geojson-vt": "^3.2.1",
    "geopackage": "^1.0.2",
    "gtfs-realtime-bindings": "^1.1.1",
    "js-priority-queue": "^0.1.5",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "mongodb": "^6.18.0",
    "mongoose": "^9.0.0",
    "morgan": "^1.10.1",
    "proj4": "^2.9.2",
    "stream-json": "^1.9.1",
    "unzipper": "^0.12.3",
    "vt-pbf": "^3.1.1",
    "xmldom": "^0.6.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
